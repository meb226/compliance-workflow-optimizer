{
  "version": 2,
  "builds": [
    {
      "src": "backend-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "dest": "backend-server.js"
    },
    {
      "src": "/",
      "dest": "public/compliance_optimizer.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}